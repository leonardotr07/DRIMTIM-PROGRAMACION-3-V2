<?xml version="1.0" encoding="utf-8"?>
<xs:schema xmlns:tns="http://prendas.services.weardrop.pucp.edu.pe/" targetNamespace="http://prendas.services.weardrop.pucp.edu.pe/" version="1.0" xmlns:xs="http://www.w3.org/2001/XMLSchema">
  <xs:element name="eliminarPantalon" type="tns:eliminarPantalon" />
  <xs:element name="eliminarPantalonResponse" type="tns:eliminarPantalonResponse" />
  <xs:element name="filtrarPantalones" type="tns:filtrarPantalones" />
  <xs:element name="filtrarPantalonesResponse" type="tns:filtrarPantalonesResponse" />
  <xs:element name="insertarPantalon" type="tns:insertarPantalon" />
  <xs:element name="insertarPantalonResponse" type="tns:insertarPantalonResponse" />
  <xs:element name="listarPantalones" type="tns:listarPantalones" />
  <xs:element name="listarPantalonesResponse" type="tns:listarPantalonesResponse" />
