  <xs:element name="obtenerPantalonPorIdResponse" type="tns:obtenerPantalonPorIdResponse" />
  <xs:complexType name="obtenerPantalonPorId">
    <xs:sequence>
      <xs:element name="idPantalon" type="xs:int" />
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="obtenerPantalonPorIdResponse">
    <xs:sequence>
      <xs:element minOccurs="0" name="return" type="tns:pantalon" />
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="pantalon">
    <xs:complexContent mixed="false">
      <xs:extension base="tns:prenda">
        <xs:sequence>
          <xs:element name="cintura" type="xs:double" />
          <xs:element name="elasticidad" type="xs:boolean" />
          <xs:element name="largoPierna" type="xs:double" />
          <xs:element minOccurs="0" name="tipoPantalon" type="tns:tipoPantalon" />
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="prenda" abstract="true">
    <xs:sequence>
      <xs:element name="activo" type="xs:boolean" />
      <xs:element name="alertaMinStock" type="xs:int" />
      <xs:element minOccurs="0" name="color" type="xs:string" />
      <xs:element minOccurs="0" maxOccurs="unbounded" name="descuentos" nillable="true" type="tns:descuento" />
      <xs:element name="idPrenda" type="xs:int" />
      <xs:element minOccurs="0" maxOccurs="unbounded" name="lotes" nillable="true" type="tns:lote" />
