      <xs:element minOccurs="0" name="elasticidad" type="xs:boolean" />
      <xs:element minOccurs="0" name="precioMin" type="xs:double" />
      <xs:element minOccurs="0" name="precioMax" type="xs:double" />
      <xs:element minOccurs="0" name="largoMin" type="xs:double" />
      <xs:element minOccurs="0" name="largoMax" type="xs:double" />
      <xs:element minOccurs="0" name="cinturaMin" type="xs:double" />
      <xs:element minOccurs="0" name="cinturaMax" type="xs:double" />
      <xs:element minOccurs="0" name="soloActivos" type="xs:boolean" />
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="filtrarPantalonesResponse">
    <xs:sequence>
      <xs:element minOccurs="0" maxOccurs="unbounded" name="return" type="tns:pantalon" />
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="modificarPantalon">
    <xs:sequence>
      <xs:element minOccurs="0" name="pantalon" type="tns:pantalon" />
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="modificarPantalonResponse">
    <xs:sequence>
      <xs:element name="return" type="xs:int" />
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="eliminarPantalon">
    <xs:sequence>
      <xs:element name="idPantalon" type="xs:int" />
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="eliminarPantalonResponse">
    <xs:sequence>
      <xs:element name="return" type="xs:int" />
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="listarPantalones">
    <xs:sequence />
  </xs:complexType>
  <xs:complexType name="listarPantalonesResponse">
    <xs:sequence>
      <xs:element minOccurs="0" maxOccurs="unbounded" name="return" type="tns:pantalon" />
    </xs:sequence>
  </xs:complexType>
