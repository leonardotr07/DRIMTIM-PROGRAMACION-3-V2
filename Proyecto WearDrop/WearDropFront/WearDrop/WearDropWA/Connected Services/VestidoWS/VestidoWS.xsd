<?xml version="1.0" encoding="utf-8"?>
<xs:schema xmlns:tns="http://prendas.services.weardrop.pucp.edu.pe/" targetNamespace="http://prendas.services.weardrop.pucp.edu.pe/" version="1.0" xmlns:xs="http://www.w3.org/2001/XMLSchema">
  <xs:element name="eliminarVestido" type="tns:eliminarVestido" />
  <xs:element name="eliminarVestidoResponse" type="tns:eliminarVestidoResponse" />
  <xs:element name="filtrarVestidos" type="tns:filtrarVestidos" />
  <xs:element name="filtrarVestidosResponse" type="tns:filtrarVestidosResponse" />
  <xs:element name="insertarVestido" type="tns:insertarVestido" />
  <xs:element name="insertarVestidoResponse" type="tns:insertarVestidoResponse" />
  <xs:element name="listarVestidos" type="tns:listarVestidos" />
  <xs:element name="listarVestidosResponse" type="tns:listarVestidosResponse" />
  <xs:element name="modificarVestido" type="tns:modificarVestido" />
  <xs:element name="modificarVestidoResponse" type="tns:modificarVestidoResponse" />
  <xs:element name="obtenerVestidoPorId" type="tns:obtenerVestidoPorId" />
